{
    "data_model": {
        "data_type": {
            // whether selected value can be multi or not
            multi_choices: true,
            // value that is accepted
            "accepted_values": [
                // NOTE: for vector data type, vector database will be selected
                // Vector database: FAISS, Deep Lake, ...
                "Vector",
                // NOTE: for SQL v√† NoSQL database, basic type of data will be:
                // text, number, datatime, boolean, binary.
                // Some will support storing objects.
                // SQL and NoSQL database
                "Text",
                "Number",
                "Datetime",
                "Boolean",
                "Binary",
                "JSON",
                "XML"// complex data structures, such as JSON
            ],
            "description": "Which type of data that you want to save into your DB?"
        },
        "structured_data": {
            // condition where this need to be asked
            "conditions": {
                "data_model": {
                    "data_type": ["Text", "Number", "Datetime", "Boolean", "Binary", "Objects"]
                }
            },
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Is your data structured?"
        },
        "time_series": {
            "conditions": {
                "data_model": {
                    "data_type": ["Text", "Number", "Datetime", "Boolean", "Binary", "Objects"]
                }
            },
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Time series data is a collection of observations obtained through repeated measurements over time. Is your data time series data?"
        },
        "relationship_centric": {
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Do you have a lot of complicated relationship between data?"
        }
    },
    "requirements": {
        "volume":  {
            multi_choices: true,
            "accepted_values": [
                "small",
                "medium",
                "large"
            ],
            "description": "How much data do you need to store? Less than 1 million records is small. Within 1 million and 100 million is medium. And above 100 million records is large."
        },
        "complex_query_patterns": {
            "conditions": {
                "data_model": {
                    "data_type": ["Text", "Number", "Datetime", "Boolean", "Binary", "Objects"]
                }
            },
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Do you have to do a lot of complex queries?"
        },
        "read_consistency": {
            multi_choices: false,
            // consistency in CAD
            "accepted_values": [
                true,
                false
            ],
            "description": "Is read consistency (any read request will return the most recent write) one of the most important requirements?"
        },
        "high_write_workloads": {
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Do you have a high-write workloads?"
        },
        "high_availability": {
            multi_choices: false,
            // availability in CAD
            "accepted_values": [
                true,
                false
            ],
            "description": "Can you accept less than 99.9% uptime for database?"
        }
    },
    "cost": {
        "commercial_allow": {
            multi_choices: false,
            "accepted_values": [
                true,
                false
            ],
            "description": "Are you willing to pay some money to use commercial database?"
        }
    }
}
